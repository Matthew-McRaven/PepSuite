{
  "name": "@pep10/core",
  "main": "index.js",
  "version": "1.0.5",
  "description": "Core library implementing simulator and assembler for Pep/10 and others.",
  "config": {
    "unsafe-perm": true
  },
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/pep10/pepsuite.git"
  },
  "scripts": {
    "start": "node index.js",
    "wasm:clean": "rimraf build",
    "wasm:prebuild": "mkdir -p build && echo '{\"name\":\"build\"}' > build/package.json",
    "wasm:build:js": "yarn wasm:prebuild && bash ./build-js.sh",
    "wasm:dist:lib": "node scripts/build_out_exists.cjs && (npx babel build/packages/core --out-dir dist --copy-files)",
    "wasm:dist:index": "npx babel index.template.js --out-file index.js",
    "wasm:dist": "yarn wasm:dist:lib && yarn wasm:dist:index"
  },
  "author": "Matthew McRaven",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://gitlab.com/pep10/pepsuite/issues"
  },
  "homepage": "https://gitlab.com/pep10/pepsuite#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4"
  }
}