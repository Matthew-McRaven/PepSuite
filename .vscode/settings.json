{
  "files.exclude": {
    "**/node_modules": true,
    "**/plugins": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "editor.formatOnSave": true,
  "eslint.run": "onSave",
  "eslint.validate": [
    "json"
  ],
  "files.associations": {
    "*.tpp": "cpp",
    "format": "cpp",
    "complex": "cpp",
    "any": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "strstream": "cpp",
    "bit": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "chrono": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "codecvt": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "condition_variable": "cpp",
    "coroutine": "cpp",
    "csetjmp": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "map": "cpp",
    "set": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "regex": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "fstream": "cpp",
    "future": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "ranges": "cpp",
    "shared_mutex": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "stop_token": "cpp",
    "streambuf": "cpp",
    "thread": "cpp",
    "cfenv": "cpp",
    "cinttypes": "cpp",
    "typeindex": "cpp",
    "typeinfo": "cpp",
    "valarray": "cpp",
    "variant": "cpp",
    "*.ipp": "cpp",
    "numbers": "cpp",
    "semaphore": "cpp",
    "__config": "cpp",
    "__config_site": "cpp"
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, IndentWidth: 4, ColumnLimit: 120 }",
  "C_Cpp.clang_format_path": "",
  "cquery.cacheDirectory": "${workspaceFolder}/.vscode/cquery_cached_index/",
  "cmake.configureOnOpen": false,
  "json.format.enable": false,
  "C_Cpp.experimentalFeatures": "Enabled",
  "C_Cpp.intelliSenseEngine": "Tag Parser",
  "C_Cpp.intelliSenseEngineFallback": "Enabled",
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "jest.showCoverageOnLoad": true,
  "jest.jestCommandLine": "yarn node --experimental-vm-modules --no-warnings $(yarn bin jest)",
  "jest.autoRun": {
    "watch":true,
    "onStartup": []
  }
}