{
  "name": "electron-sample",
  "version": "1.0.4",
  "main": "build/electron/main.js",
  "description": "",
  "dependencies": {
    "@pep10/ui-sample": "file:../../packages/ui-sample",
    "@types/electron-devtools-installer": "^2.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "react": "~17.0.0",
    "react-dom": "~17.0.0",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@craco/craco": "~6.4.0",
    "@types/node": "~16.11.0",
    "@types/react": "~17.0.0",
    "@types/react-dom": "~17.0.0",
    "concurrently": "^6.4.0",
    "cross-env": "~7.0.2",
    "electron": "^16.0.1",
    "electron-builder": "^22.14.5",
    "fork-ts-checker-webpack-plugin": "~6.4.0",
    "react-scripts": "~4.0.1",
    "tsconfig-paths-webpack-plugin": "~3.5.0",
    "wait-on": "^6.0.0"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true craco start",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true craco build",
    "test": "react-scripts test",
    "postinstall": "npx electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm run build && tsc -p electron && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/pep10/pepsuite.git"
  },
  "author": "Matthew McRaven",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://gitlab.com/pep10/pepsuite/issues"
  },
  "homepage": ".",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
